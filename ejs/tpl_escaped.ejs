<html>
	<head>
		<title><%= title %></title>
	</head>
	<body>
		<p><%= text %></p>
		<% if (projects.length) { %>
			<% for (var i = 0; i < projects.length; i++) { %>
				<a href="<%= projects[i].url %>"><%= projects[i].name %></a>
				<p><%= projects[i].description %></p>
			<% } %>
		<% } else { %>
			No projects
		<% } %>
	</body>
</html>